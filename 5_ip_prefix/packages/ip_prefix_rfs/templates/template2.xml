<config-template xmlns="http://tail-f.com/ns/config/1.0">
  <devices xmlns="http://tail-f.com/ns/ncs">
    <device>
      <name>{/device}</name>
      <config>
        <ip xmlns="http://tail-f.com/ned/cisco-nx">
          <prefix-list>
            <prefixes>
              <name>{$IP_PREFIX_NAME}</name>
              <seq>
                <no>{$SEQ}</no>
                <permit>{$PERMIT}</permit>
                <le>{$LE}</le>
              </seq>
            </prefixes>
          </prefix-list>
        </ip>
      </config>
    </device>
  </devices>
</config-template>
